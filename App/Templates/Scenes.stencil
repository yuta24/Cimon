import Foundation
import UIKit
import Shared

enum Scenes {

    {% for type in types.all %}
    {% if type|annotated:"scene" %}
    static var {{type.name|replace:"ViewController",""|lowerFirstWord}}: Reader<{{type.name}}.Dependency, {{type.name}}> {
        return .init({ dependency in
            return Storyboard<{{type.name}}>(name: "{{type.name|replace:"ViewController",""}}")
                .instantiate(dependency: dependency)
         })
    }

    {% endif %}
    {% endfor %}
}
